rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /rsvps/{document=**} {
      allow read: if false;
      allow write: if request.time < timestamp.date(2026, 1, 1);
    }

    match /invitees/{document=**} {
      allow read: if true;
      allow write: if false;
    }

    match /{document=**} {
      allow read, write: if false;
    }
  }
}
